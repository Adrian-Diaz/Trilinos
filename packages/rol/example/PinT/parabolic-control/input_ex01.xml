<ParameterList name="Example 01">
  <!--================================================================================--> 
  <!-- Physical Parameters                                                            --> 
  <Parameter name="Diffusion Scale"         type="double"   value="0.5"   />
  <Parameter name="Reaction Scale"          type="double"   value="1.0"   />
  <Parameter name="Reaction Type"           type="int"      value="1"     />
  <!--================================================================================--> 
  <!-- Space Discretization                                                           --> 
  <Parameter name="Spatial Discretization"  type="int"      value="64"    />
  <!--================================================================================-->
  <!--================================================================================--> 
  <!-- Time Discretization                                                            --> 
  <Parameter name="End Time"                type="double"   value="1.0"   />
  <Parameter name="Temporal Discretization" type="int"      value="100"   />
  <Parameter name="Integration Factor"      type="double"   value="0.5"   />
  <!--================================================================================-->
  <!-- Objective Function                                                             --> 
  <Parameter name="Target Type"             type="int"      value="2"     />
  <Parameter name="Control Penalty"         type="double"   value="1e-3"  />
  <!--================================================================================--> 
  <!-- Sketching Parameters                                                           --> 
  <Parameter name="Use Sketching"           type="bool"     value="false" />
  <Parameter name="Sketch Rank"             type="int"      value="10"    />
  <!--================================================================================-->
</ParameterList>


